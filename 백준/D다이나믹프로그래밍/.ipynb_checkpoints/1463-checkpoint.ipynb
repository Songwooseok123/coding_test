    "n = int(input())\n",
    "a = [0]*(n+1)\n",
    "a[0] =False\n",
    "a[1] = 0\n",
    "\n",
    "\n",
    "for i in range(2,n+1):\n",
    "    a[i] = a[i-1]+1\n",
    "\n",
    "    if i %2 ==0:\n",
    "        a[i] = min(a[i], 1+a[int(i/2)])\n",
    "    if i %3 ==0:\n",
    "        a[i] = min(a[i],1+a[int(i/3)])\n",
    "print(a[-1])\n",
    "    "
